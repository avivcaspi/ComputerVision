function I_warped = warpImage(I,H)
I_r = I(:,:,1); % Red channel
I_g = I(:,:,2); % Green channel
I_b = I(:,:,3); % Blue channel

I_warped = padarray(zeros(size(I_r)),100);
size(I_warped)
for u=1:size(I_r,1)
    for v=1:size(I_r,2)
        indices = H * [u, v, 1]';
        indices = indices ./ indices(3);
        x = indices(1) + 50;
        y = indices(2) + 50
        
        I_warped(round(x),round(y)) = I_r(u,v);
    end
end
end

